brute force approch

int sort(vector<int> &arr){
int n = arr.size();
sort(arr.begin(), arr.end();
}
return arr[n-1];



optimised method

int findLargestElement(int arr[], int n) {
 
  int max = arr[0];
  for (int i = 0; i < n; i++) {
    if (max < arr[i]) {
      max = arr[i];
    }
  }
  return max;
}

